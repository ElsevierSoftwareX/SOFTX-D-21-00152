@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
:Mineral a owl:Class .
:Mineral :hasProperty :Name .
:Name a owl:Property .
:Name :hasDatatype xsd:String .
:Name :hasValue :-; .
:Mineral :hasProperty :Color .
:Color a owl:Property .
:Color :hasDatatype xsd:String .
:Color :hasValue :Transparent;Green; .
:Hardness-(Moos) a owl:Class .
:Hardness-(Moos) :hasProperty :Density .
:Density a owl:Property .
:Density :hasDatatype xsd:String .
:Density :hasValue :3,47-3,55;2,69-2,78; .
:Hardness-(Moos) :hasProperty :Value .
:Value a owl:Property .
:Value :hasDatatype xsd:String .
:Value :hasValue :10;7,5-8; .
:Transparency a owl:Class .
:Transparency :hasProperty :Bandwidth .
:Bandwidth a owl:Property .
:Bandwidth :hasDatatype xsd:String .
:Bandwidth :hasValue :Transparent;Transparent, semi-transparent; .
:Transparency :hasProperty :Refractive-index .
:Refractive-index a owl:Property .
:Refractive-index :hasDatatype xsd:String .
:Refractive-index :hasValue :2,417-2,419;1,56-1,6; .
:Syngony a owl:Class .
:Syngony :hasProperty :System .
:System a owl:Property .
:System :hasDatatype xsd:String .
:System :hasValue :Cubic;Hexagonal; .
:Syngony :hasProperty :Appearance-of-crystals .
:Appearance-of-crystals a owl:Property .
:Appearance-of-crystals :hasDatatype xsd:String .
:Appearance-of-crystals :hasValue :Octahedral;Massive; .
:Hardness-(Moos) :hasRelationship :Mineral .
:Transparency :hasRelationship :Mineral .
:Syngony :hasRelationship :Mineral .
